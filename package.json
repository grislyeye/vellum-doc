{
  "name": "vellum-doc",
  "private": true,
  "version": "0.2.0",
  "description": "A simple web component",
  "main": "vellum-doc.js",
  "module": "vellum-doc.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf vellum-doc.{d.ts,d.ts.map,js,js.map} test/vellum-doc.{d.ts,d.ts.map,js,js.map} test/vellum-doc_test.{d.ts,d.ts.map,js,js.map}",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "serve": "wds --watch",
    "serve:prod": "MODE=prod npm run serve",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "wtr",
    "test:watch": "wtr --watch",
    "test:prod": "MODE=prod wtr",
    "test:prod:watch": "MODE=prod wtr --watch",
    "checksize": "rollup -c ; cat vellum-doc.bundled.js | gzip -9 | wc -c ; rm vellum-doc.bundled.js"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit",
    "publishing",
    "vellum"
  ],
  "author": "Ric Wood <ric@grislyeye.com>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "lit": "^3.1.2"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.9.4",
    "@open-wc/testing": "^4.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@types/mocha": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@web/dev-server": "^0.4.3",
    "@web/dev-server-legacy": "^2.1.0",
    "@web/test-runner": "^0.18.1",
    "@web/test-runner-playwright": "^0.11.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "eslint": "^8.57.0",
    "lit-analyzer": "^2.0.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.13.2",
    "typescript": "~5.4.3"
  },
  "customElements": "custom-elements.json"
}
